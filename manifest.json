{
    "name": "Trackpad Gestures",
    "version": "0.1",
    "author": "SamTS",
    "description": "An extension that allows you to browse the web productively with your laptop's trackpad and keyboard",
    "permissions": ["activeTab", "storage"],
    "background":{
        "scripts":["/static/background.js"],
        "persistent": false
    
    },
    "page_action":{
        "default_popup": "views/popup.html",
        "default_icon":{
            "16":"icons/Golf_ball.png"
        }
    },
    "options_page": "views/options.html",
    "content_scripts":[
        {
            "js": ["static/contentScript.js"]
        }
    ],
    "manifest_version": 2
}